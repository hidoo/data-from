{
  "name": "@hidoo/data-from",
  "version": "0.3.0",
  "description": "A utility library that get data from JSON, JSON5, and YAML.",
  "engines": {
    "node": ">=10.0.0"
  },
  "main": "lib/index.js",
  "files": [
    "lib/",
    "README.md"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production babel ./src --out-dir ./lib",
    "test": "npm-run-all -s test:*",
    "test:lint": "eslint .",
    "test:unit": "cross-env NODE_ENV=test mocha ./test",
    "prepare": "npm-run-all -s test build",
    "version": "npm-run-all -s prepare version:changelog version:commit",
    "version:changelog": "conventional-changelog -p angular -i ./CHANGELOG.md -s -r 0",
    "version:commit": "git add ./CHANGELOG.md"
  },
  "keywords": [
    "data",
    "yaml",
    "json5"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hidoo/data-from.git"
  },
  "author": "hidoo",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.13.10",
    "@babel/core": "7.13.13",
    "@babel/preset-env": "7.13.12",
    "@babel/register": "7.13.8",
    "@commitlint/cli": "12.0.1",
    "@commitlint/config-conventional": "12.0.1",
    "@hidoo/eslint-config": "0.6.0",
    "babel-preset-power-assert": "3.0.0",
    "conventional-changelog-cli": "2.1.1",
    "cross-env": "7.0.3",
    "eslint": "7.23.0",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "mocha": "8.3.2",
    "npm-run-all": "4.1.5",
    "power-assert": "1.6.1"
  },
  "dependencies": {
    "front-matter": "4.0.2",
    "glob": "7.1.6",
    "handlebars": "4.7.7",
    "js-yaml": "3.14.1",
    "json5": "2.2.0",
    "lodash.merge": "4.6.2"
  },
  "resolutions": {
    "minimist": "^1.2.5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "eslint"
    ]
  }
}
