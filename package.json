{
  "name": "@hidoo/data-from",
  "version": "0.3.1",
  "description": "Utility to get data from JSON, JSON5, and YAML.",
  "packageManager": "pnpm@8.15.5",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": "8.15.5"
  },
  "type": "module",
  "main": "src/index.js",
  "exports": {
    ".": "./src/index.js"
  },
  "files": [
    "src/"
  ],
  "scripts": {
    "format": "prettier . --write",
    "preinstall": "npx only-allow pnpm",
    "prepare": "git config --local core.hooksPath .githooks",
    "test": "pnpm run --sequential '/^test:.*/'",
    "test:lint": "eslint .",
    "test:unit": "cross-env NODE_ENV=test mocha ./test",
    "version": "pnpm run --sequential '/^(test|version:[^:]+)$/'",
    "version:changelog": "conventional-changelog -p angular -i ./CHANGELOG.md -s -r 0",
    "version:commit": "git add ./CHANGELOG.md"
  },
  "keywords": [
    "data",
    "yaml",
    "json5"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hidoo/data-from.git"
  },
  "author": "hidoo",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "19.2.1",
    "@commitlint/config-conventional": "19.1.0",
    "@hidoo/eslint-config": "1.3.1",
    "conventional-changelog-cli": "4.1.0",
    "cross-env": "7.0.3",
    "eslint": "8.57.0",
    "lint-staged": "15.2.2",
    "mocha": "10.3.0",
    "prettier": "3.2.5"
  },
  "dependencies": {
    "front-matter": "4.0.2",
    "glob": "10.3.10",
    "handlebars": "4.7.8",
    "js-yaml": "4.1.0",
    "json5": "2.2.3",
    "lodash.merge": "4.6.2"
  }
}
