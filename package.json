{
  "name": "@hidoo/data-from",
  "version": "0.3.1",
  "description": "A utility library that get data from JSON, JSON5, and YAML.",
  "packageManager": "pnpm@8.15.5",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": "8.15.5"
  },
  "main": "lib/index.js",
  "files": [
    "lib/",
    "README.md"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production babel ./src --out-dir ./lib",
    "format": "prettier . --write",
    "preinstall": "npx only-allow pnpm",
    "prepare": "git config --local core.hooksPath .githooks",
    "test": "pnpm run --sequential '/^test:.*/'",
    "test:lint": "eslint .",
    "test:unit": "cross-env NODE_ENV=test mocha ./test",
    "version": "pnpm run --sequential '/^(test|version:[^:]+)$/'",
    "version:changelog": "conventional-changelog -p angular -i ./CHANGELOG.md -s -r 0",
    "version:commit": "git add ./CHANGELOG.md"
  },
  "keywords": [
    "data",
    "yaml",
    "json5"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hidoo/data-from.git"
  },
  "author": "hidoo",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.14.8",
    "@babel/core": "7.14.8",
    "@babel/preset-env": "7.14.9",
    "@babel/register": "7.14.5",
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@hidoo/eslint-config": "0.6.0",
    "babel-preset-power-assert": "3.0.0",
    "conventional-changelog-cli": "2.1.1",
    "cross-env": "7.0.3",
    "eslint": "7.30.0",
    "husky": "6.0.0",
    "lint-staged": "11.0.1",
    "mocha": "9.0.3",
    "prettier": "3.2.5",
    "power-assert": "1.6.1"
  },
  "dependencies": {
    "front-matter": "4.0.2",
    "glob": "7.1.7",
    "handlebars": "4.7.8",
    "js-yaml": "4.1.0",
    "json5": "2.2.3",
    "lodash.merge": "4.6.2"
  },
  "resolutions": {
    "minimist": "^1.2.5"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint"
    ]
  }
}
